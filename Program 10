import time

def timer_decorator(func):
    def wrapper(*args):
        start = time.time()
        result = list(func(*args))
        end = time.time()
        print(f"Time Taken: {end - start:.6f} seconds")
        return result
    return wrapper

@timer_decorator
def fib_gen(n):
    a, b = 0, 1
    for i in range(n):
        yield a
        a, b = b, a + b

n = 10
print(f"Fibonacci Series ({n} terms):", fib_gen(n))











